{
  "name": "casadituzza",
  "version": "0.0.2",
  "private": true,
  "description": "casadituzza",
  "author": "Igor Patro",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint": "eslint --fix",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write ./src",
    "api-dev": "set NODE_ENV=development&& nodemon src/api/src/index.ts",
    "api-build": "tsc --project src/api/",
    "api-start": "node src/api/dist/index.js"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.13.3",
    "gatsby": "^4.1.3",
    "gatsby-plugin-image": "^2.1.2",
    "gatsby-plugin-manifest": "^4.1.3",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-sharp": "^4.1.3",
    "gatsby-plugin-styled-components": "^5.1.0",
    "gatsby-plugin-typescript": "^4.1.2",
    "gatsby-source-filesystem": "^4.1.2",
    "gatsby-transformer-sharp": "^4.1.0",
    "js-base64": "^3.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "eslint-config-prettier": "^8.3.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "yarn run lint",
      "yarn run format",
      "git add ."
    ]
  }
}
